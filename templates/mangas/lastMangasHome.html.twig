{% for manga in lastMangas %}
    <div class="containerManga">
        <div class="mangas">
            <a href="#">
                <img src="{{ manga.coverTome }}" alt="Image d'accueil de {{ manga.nameTome }}" title="Image du manga {{ manga.nameTome }}" />
            </a>
            <h3>{{ manga.nameTome }}<br/>
            {{ manga.idManga.nameManga }}</h3>
        </div>
    </div>
{% endfor %}